    </script>
    <script src ="http://cdnjs.cloudflare.com/ajax/libs/sugar/1.3.9/sugar.min.js">
    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBhAGyvGAvEZeF0TdH5zVy9sQtEIXRLeuo&sensor=true">
    </script>

    <%= javascript_include_tag "application" %>
